		{
			SPLINE SWEEPOBJECT_SPLINESCALE { }
			SPLINE SWEEPOBJECT_SPLINEROTATION { }

			GROUP
			{
				COLUMNS 2;
				REAL SWEEPOBJECT_ROTATIONFROM	{ UNIT DEGREE; }
				REAL SWEEPOBJECT_ROTATIONTO	{ UNIT DEGREE; }
			}
		}
	}
}
