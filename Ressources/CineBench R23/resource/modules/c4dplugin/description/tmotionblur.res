CONTAINER Tmotionblur
{
	NAME Tmotionblur;
	INCLUDE Tbase;

	GROUP ID_TAGPROPERTIES
	{
		REAL MOTIONBLURTAG_STRENGTH { UNIT PERCENT; MIN 0.0; MAX 1000.0; }

		SEPARATOR { LINE; }

		LONG MOTIONBLURTAG_MODE
		{
			CYCLE
			{
				MOTIONBLURTAG_MODE_FULL;
				MOTIONBLURTAG_MODE_MOTION;
				MOTIONBLURTAG_MODE_NONE;
			}
		}

	}
}
