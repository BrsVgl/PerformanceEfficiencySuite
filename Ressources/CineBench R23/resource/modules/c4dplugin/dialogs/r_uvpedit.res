// C4D-DialogResource// Numerical editor for UV pointDIALOG R_UVPEDIT{	GROUP	{		COLUMNS 2; 		CENTER_H;				STATICTEXT { NAME T2; FIT_H;}		EDITNUMBERARROWS IDC_U { SIZE 100,0; FIT_H;}		STATICTEXT { NAME T3; FIT_H;}		EDITNUMBERARROWS IDC_V { SIZE 100,0; FIT_H; }		STATICTEXT { NAME T4; FIT_H;}		EDITNUMBERARROWS IDC_W { SIZE 100,0; FIT_H;}	}	DLGGROUP { OK;CANCEL;HELP; }}