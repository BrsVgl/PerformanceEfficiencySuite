			{
				PARENTCOLLAPSE ID_CA_MUSCLE_DEFORMER_PULL_MODE;
			}					
		}
		
		GROUP ID_CA_MUSCLE_DEFORMER_STRETCH_GROUP
		{
			COLUMNS 1; DEFAULT 1;
			
