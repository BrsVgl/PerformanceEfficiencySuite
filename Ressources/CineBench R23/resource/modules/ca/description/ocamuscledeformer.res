			REAL ID_CA_MUSCLE_DEFORMER_SLIDE_STRENGTH { UNIT PERCENT; MINSLIDER 0.0; MAXSLIDER 100.0; CUSTOMGUI REALSLIDER; }
			REAL ID_CA_MUSCLE_DEFORMER_FAT_OFFSET { UNIT METER; MIN 0.0; MAXSLIDER 100.0; CUSTOMGUI REALSLIDER; }			
		}
			
		GROUP ID_CA_MUSCLE_DEFORMER_PULL_GROUP
		{
			DEFAULT 1;
			
			GROUP
			{
				COLUMNS 2;
				
				BUTTON ID_CA_MUSCLE_DEFORMER_INIT { }
				BUTTON ID_CA_MUSCLE_DEFORMER_FREE { }
				
				REAL ID_CA_MUSCLE_DEFORMER_DISTANCE { UNIT METER; MIN 0.0; MAXSLIDER 1000.0; CUSTOMGUI REALSLIDER; SCALE_H; }
