			REAL ID_CA_MUSCLE_DEFORMER_STRETCH_STRENGTH { UNIT PERCENT; MIN 0.0; MAX 100.0; CUSTOMGUI REALSLIDER; }
			LONG ID_CA_MUSCLE_DEFORMER_STRETCH_ITERATIONS { MIN 1; }
			REAL ID_CA_MUSCLE_DEFORMER_STRETCH_STIFFNESS { UNIT PERCENT; MIN 0.0; MAX 100.0; CUSTOMGUI REALSLIDER; }
		}
	}
	
	GROUP ID_CA_MUSCLE_DEFORMER_MUSCLES_GROUP
	{	
		SCALE_V;
		
		IN_EXCLUDE ID_CA_MUSCLE_DEFORMER_OBJECT_LIST
		{
			NUM_FLAGS 0; INIT_STATE 0; SEND_SELCHNGMSG 1; SCALE_V; ANIM OFF;
			ACCEPT { 1026224; };
		}		
	}	
