CONTAINER Oshrinkwrapdeformer
{
	NAME Oshrinkwrapdeformer;
	INCLUDE Obase;
	
	GROUP ID_OBJECTPROPERTIES
	{
		LINK SHRINKWRAP_TARGETOBJECT
		{
			ACCEPT
			{
				Obase;
			}
		}
		LONG SHRINKWRAP_MODE
		{
			CYCLE
			{
				SHRINKWRAP_MODE_NORMALS;
				SHRINKWRAP_MODE_TAXIS;
				SHRINKWRAP_MODE_SAXIS;
			}
		}
		REAL SHRINKWRAP_STRENGTH
		{
			UNIT PERCENT;
			MINSLIDER 0.0;
			MAXSLIDER 100.0;
			STEP 0.1;
			CUSTOMGUI REALSLIDER;
		}
		REAL SHRINKWRAP_MAXDISTANCE
		{
			UNIT METER;
			MIN 0.0;
		}
	}
}
