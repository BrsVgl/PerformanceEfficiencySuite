		GVGENERALOBJECT GV_HIERARCHY_OUTPUT_OBJECT { OUTPORT; STATICPORT; CREATEPORT; ITERATOR; }
		LONG GV_HIERARCHY_OUTPUT_COUNT { OUTPORT; }
	}
}
