		IN_EXCLUDE GV_HIERARCHY_EXCLUDE_ID { SCALE_V; NUM_FLAGS 0; INIT_STATE 0; IMAGE_01_ON 1009316; IMAGE_01_OFF 1009320; ACCEPT { Obase; } }
	}

	GROUP ID_GVPORTS
	{
		GVGENERALOBJECT GV_HIERARCHY_INPUT_OBJECT { INPORT; }
		LONG GV_HIERARCHY_INPUT_MAX { INPORT; EDITPORT; MIN 0; }
