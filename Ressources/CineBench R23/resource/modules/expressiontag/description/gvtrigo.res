CONTAINER GVtrigo
{
	NAME GVtrigo;
	INCLUDE GVbase;

	GROUP ID_GVPROPERTIES
	{
		LONG GV_TRIGO_FUNC 
		{
			CYCLE
			{
				TYPE_SIN;
				TYPE_COS;
				TYPE_TAN;
				TYPE_SINH;
				TYPE_COSH;
				TYPE_TANH;
				TYPE_ASIN;
				TYPE_ACOS;
				TYPE_ATAN;
			}
		}
	}

	GROUP ID_GVPORTS
	{
		REAL GV_TRIGO_INPUT  { INPORT;  STATICPORT; CREATEPORT; }
		REAL GV_TRIGO_OUTPUT { OUTPORT; STATICPORT; CREATEPORT; }
	}
}
