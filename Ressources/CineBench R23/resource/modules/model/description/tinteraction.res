				{
					INTERACTIONTAG_SCRIPT_LANGUAGE_NONE;
					INTERACTIONTAG_SCRIPT_LANGUAGE_PYTHON;
					INTERACTIONTAG_SCRIPT_LANGUAGE_COFFEE;
				}
			}
		}

		GROUP INTERACTIONTAG_GROUP_SCRIPT
		{
			FIT_H;

			STRING		INTERACTIONTAG_SCRIPT_PY { ANIM OFF; PYTHON; CUSTOMGUI MULTISTRING; SCALE_V; SCALE_H; }
STRING		INTERACTIONTAG_SCRIPT_COF { ANIM OFF; CUSTOMGUI MULTISTRING; READONLY; SCALE_V; SCALE_H; }
			BUTTON		INTERACTIONTAG_EDIT_SCRIPT { ANIM OFF; }
		}
