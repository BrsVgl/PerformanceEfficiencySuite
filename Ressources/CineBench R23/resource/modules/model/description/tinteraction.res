				FIT_H;

				BOOL		INTERACTIONTAG_TWEAKABLE { ANIM OFF; SCALE_H; }
				BOOL		INTERACTIONTAG_SELECT { ANIM OFF; SCALE_H; }

				LONG		INTERACTIONTAG_SELECTEDTWEAKMODE 
