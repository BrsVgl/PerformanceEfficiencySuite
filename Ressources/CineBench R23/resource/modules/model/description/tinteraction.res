					} 
				}
				BOOL		INTERACTIONTAG_DISABLEMOVE { ANIM OFF; SCALE_H; }
			}
			SEPARATOR INTERACTIONTAG_GROUP_DISPLAY { }
			GROUP
			{
				COLUMNS 2;
				FIT_H;

				BOOL		INTERACTIONTAG_HIGHLIGHT { ANIM OFF; SCALE_H; }
				BOOL		INTERACTIONTAG_HIDE_MOUSE { ANIM OFF; SCALE_H; }
				BOOL		INTERACTIONTAG_HIDE_OBJECT { ANIM OFF; SCALE_H; }
			}
		}
	}
	
